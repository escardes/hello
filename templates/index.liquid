

{% assign collection = collections['all'] %}

{{ collection }}

{{ collection.title }}
<ul>
  {% for val in collection.products %}
    <li> <a href = "{{ val.url }}">{{ val.title }}-
      {{ val.price |money }}</a>
      {% if val.available != true %}
        ( SOLDOUT)
      {% endif %}
    </li>
  {% endfor %}
</ul>

